{
    "preset": "laravel",
    "rules": {
        "concat_space": {
            "spacing": "one"
        },
        "ordered_imports": {
            "sort_algorithm": "length"
        },
        "not_operator_with_successor_space": false,
        "class_attributes_separation": {
            "elements": {
                "const": "none",
                "method": "one",
                "property": "none",
                "trait_import": "none",
                "case": "none"
            }
        },
        "single_line_comment_spacing": true,
        "Laravel/laravel_phpdoc_alignment": false
    }
}